{
  "Name": "ds000005",
  "BIDSModelVersion": "1.0.0",
  "Input": {
    "subject": [
      "01",
      "02",
      "03",
      "04",
      "05",
      "06",
      "07",
      "08",
      "09",
      "10",
      "11",
      "12",
      "13",
      "14",
      "15",
      "16"
    ],
    "task": [
      "mixedgamblestask"
    ]
  },
  "Nodes": [
    {
      "Level": "Run",
      "Name": "run_level",
      "GroupBy": [
        "run",
        "subject"
      ],
      "Transformations": {
        "Transformer": "pybids-transforms-v1",
        "Instructions": [
          {
            "Name": "Threshold",
            "Input": ["gain"],
            "Binarize": true,
            "Output": ["rt_reg"]
          },
          {
            "Name": "Factor",
            "Input": [
              "rt_reg"
            ]
          },
          {
            "Name": "Assign",
            "Input": ["response_time"],
            "Target": ["rt_reg.1"],
            "TargetAttr": "duration"
          },
          {
            "Name": "Convolve",
            "Model": "spm",
            "Input": [
              "parametric loss",
              "parametric gain",
              "distance from indifference",
              "rt_reg.1"
            ]
          }
        ]
      },
      "Model": {
        "X": [
          1,
          "parametric loss",
          "parametric gain",
          "distance from indifference",
          "rt_reg.1",
          "trans_*",
          "rot_*",
          "cosine*"
        ],
        "Type": "glm"
      },
      "Contrasts": [
        {
          "Name": "paragain",
          "ConditionList": [
            "parametric gain"
          ],
          "Weights": [
            1
          ],
          "Test": "t"
        },
        {
          "Name": "paragainvloss",
          "ConditionList": [
            "parametric gain",
            "parametric loss"
          ],
          "Weights": [
            1,
            -1
          ],
          "Test": "t"
        },
        {
          "Name": "paraloss",
          "ConditionList": [
            "parametric loss"
          ],
          "Weights": [
            1
          ],
          "Test": "t"
        },
        {
          "Name": "distindiff",
          "ConditionList": [
            "distance from indifference"
          ],
          "Weights": [
            1
          ],
          "Test": "t"
        },
        {
          "Name": "rt",
          "ConditionList": [
            "rt_reg.1"
          ],
          "Weights": [
            1
          ],
          "Test": "t"
        }
      ]
    },
    {
      "Level": "Subject",
      "Name": "subject_level",
      "GroupBy": [
        "subject",
        "contrast"
      ],
      "Model": {
        "X": [
          1
        ],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    },
    {
      "Level": "Dataset",
      "Name": "data_level",
      "GroupBy": [
        "contrast"
      ],
      "Model": {
        "X": [
          1
        ],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    }
  ]
}